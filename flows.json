                "e436fc8b17195df7"
            ]
        ]
    },
    {
        "id": "c6a25797451d66e1",
        "type": "function",
        "z": "5da3d01b0b78d1f5",
        "name": "Unmute Selected Chan",
        "func": "// Example inputs (you can pass these in msg or set as context/global variables)\nlet cParam1 = \"a\";\nlet cParam3 = parseInt(msg.payload); // assuming cParam3 is passed in msg.payload\n\n// Loop from 0 to 7 for cParam2\nfor (let cParam2 = 0; cParam2 < 8; cParam2++) {\n\n    let command = \"\";\n\n    if(cParam2 != cParam3) {\n        command = `SETD^${cParam1}.${cParam2}.mute^1\\r`; //MUTE ON\n    }\n    else {\n        command = `SETD^${cParam1}.${cParam2}.mute^0\\r`; //MUTE OFF\n    }\n    \n    // Send this out via another output, or collect them\n    node.send({ payload: command });\n}\nreturn null; // We already used node.send inside the loop\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
